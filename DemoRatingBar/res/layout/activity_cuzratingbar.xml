<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:rb="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.demoratingbar.MainActivity" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="50dp"
        android:gravity="center_vertical"
        android:text="TextView" />

    <com.example.custom.CuzRatingBar
        android:id="@+id/myRatingBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView1"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/textView1"
        rb:star_bg="@drawable/frm_profile_list_icon_heart_off"
        rb:star_height="45dp"
        rb:star_img="@drawable/frm_profile_list_icon_heart_on"
        rb:star_num="5"
        rb:star_rating="2.2"
        rb:star_spacing="10dp"
        rb:star_width="45dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_toRightOf="@+id/myRatingBar2"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"
        android:text="TextView" />

    <com.example.custom.CuzRatingBar
        android:id="@+id/myRatingBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="10dp"
        android:layout_alignParentLeft="true"
        rb:star_bg="@drawable/frm_profile_list_icon_heart_off"
        rb:star_height="45dp"
        rb:star_img="@drawable/frm_profile_list_icon_heart_on"
        rb:star_num="5"
        rb:star_rating="3.7"
        rb:star_width="45dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"
        android:text="TextView" />

    <com.example.custom.CuzRatingBar
        android:id="@+id/myRatingBar3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/textView3"
        rb:star_bg="@drawable/frm_profile_list_icon_heart_off"
        rb:star_height="45dp"
        rb:star_img="@drawable/frm_profile_list_icon_heart_on"
        rb:star_num="6"
        rb:star_range="3"
        rb:star_rating="3.7"
        rb:star_width="45dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView3"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"
        android:text="TextView" />

    <com.example.custom.CuzRatingBar
        android:id="@+id/myRatingBar4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/textView4"
        rb:star_bg="@drawable/frm_profile_list_icon_heart_off"
        rb:star_height="45dp"
        rb:star_img="@drawable/frm_profile_list_icon_heart_on"
        rb:star_num="5"
        rb:star_range="10"
        rb:star_rating="3.7"
        rb:star_width="45dp" />
    
    <TextView
        android:id="@+id/textView5"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_below="@+id/textView4"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"
        android:text="TextView" />

    <com.example.custom.CuzRatingBar
        android:id="@+id/myRatingBar5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@+id/textView5"
        rb:star_bg="@drawable/frm_profile_list_icon_heart_off"
        rb:star_height="45dp"
        rb:star_img="@drawable/frm_profile_list_icon_heart_on"
        rb:star_touchable="false"
        rb:star_num="5"
        rb:star_range="10"
        rb:star_rating="3.7"
        rb:star_width="45dp" />

</RelativeLayout>